<h1><%= @place.name %></h1>
<ul>
  <% @entries.each do |entry| %>
    <li>
      <h2><%= entry.title %></h2>
      <p><%= entry.posted_on %></p>
      <p><%= entry.description %></p>
    </li>
  <% end %>
</ul>
<%= link_to 'New Entry', new_place_entry_path(@place) %>
<%= link_to 'Back to Places', places_path %>
